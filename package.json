{
  "name": "fs-spartacus-bridge",
  "version": "2.1.9",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "format": "prettier --write \"./**/*.{ts,js,json,css,scss,html}\"",
    "format:check": "prettier --config ./.prettierrc --list-different \"{src,projects}/**/*.{ts,js,json,css,scss,html}\"",
    "uglify-dist": "node ./build-scripts/uglify.js ./dist/fs-spartacus-bridge --source-map",
    "lint": "ng lint",
    "lint:staged": "lint-staged -r -v",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@ng-bootstrap/ng-bootstrap": "7.0.0",
    "@ng-select/ng-select": "5.1.0",
    "@ngrx/effects": "10.1.2",
    "@ngrx/router-store": "10.1.2",
    "@ngrx/store": "10.1.2",
    "bootstrap": "4.5.2",
    "express": "4.17.1",
    "i18next": "19.9.2",
    "i18next-xhr-backend": "3.2.2",
    "immutable": "4.0.0-rc.12",
    "jquery": "3.6.0",
    "ngx-infinite-scroll": "8.0.2",
    "popper.js": "1.16.1",
    "rxjs": "6.6.7",
    "tslib": "2.0.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1002.3",
    "@angular-devkit/build-ng-packagr": "0.1002.3",
    "@angular-devkit/core": "10.2.3",
    "@angular-devkit/schematics": "10.2.3",
    "@angular/animations": "10.2.5",
    "@angular/cli": "10.2.3",
    "@angular/common": "10.2.5",
    "@angular/compiler": "10.2.5",
    "@angular/compiler-cli": "10.2.5",
    "@angular/core": "10.2.5",
    "@angular/forms": "10.2.5",
    "@angular/language-service": "10.2.5",
    "@angular/localize": "10.2.5",
    "@angular/platform-browser": "10.2.5",
    "@angular/platform-browser-dynamic": "10.2.5",
    "@angular/platform-server": "10.2.5",
    "@angular/router": "10.2.5",
    "@angular/service-worker": "10.2.5",
    "@schematics/angular": "10.2.3",
    "@spartacus/core": "3.2.1",
    "@spartacus/schematics": "3.2.1",
    "@spartacus/storefront": "3.2.1",
    "@spartacus/styles": "3.2.1",
    "@types/jasmine": "3.6.10",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.20.11",
    "angular-oauth2-oidc": "10.0.3",
    "codelyzer": "6.0.2",
    "command-line-args": "5.1.1",
    "fs-spartacus-common": "^3.1.2",
    "fs-tpp-api": "2.4.0",
    "husky": "6.0.0",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.2",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "karma-junit-reporter": "2.0.1",
    "lint-staged": "10.5.4",
    "ng-packagr": "10.1.2",
    "parse5": "6.0.1",
    "prettier": "2.2.1",
    "protractor": "7.0.0",
    "puppeteer": "9.1.0",
    "rollup": "2.47.0",
    "stylelint": "13.13.1",
    "terser": "5.7.0",
    "ts-node": "9.1.1",
    "tsickle": "0.39.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-etc": "1.13.9",
    "tsutils": "3.21.0",
    "typescript": "4.0.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint -c ./tslint.json -p ./src/tsconfig.commit.json --fix"
    ],
    "*.css": [
      "stylelint"
    ],
    "*.scss": [
      "stylelint --syntax=scss"
    ],
    "*.{js,ts,json,scss,css,html}": [
      "prettier --write"
    ]
  }
}
